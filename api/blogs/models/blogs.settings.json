{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "Blogs",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Content": {
      "type": "richtext",
      "required": true
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "Tag": {
      "type": "component",
      "repeatable": true,
      "component": "custom.tag"
    },
    "Cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    }
  }
}
